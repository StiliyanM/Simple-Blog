@model CategoriesListingModel

<tr>
    <td>
        @Html.DisplayFor(modelItem => Model.Name)
    </td>
    <td>
        <a asp-action="Edit"
           asp-route-id="@Model.Id"
           class="btn-sm btn-warning">Edit</a>
        <a asp-action="Delete"
           asp-route-id="@Model.Id"
           class="btn-sm btn-danger">Delete</a>
    </td>
</tr>

